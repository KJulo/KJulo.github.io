---
import * as message from "@paraglide/messages";
---

<div>
  <h2 class="text-2xl">🎓 {message.resume_education_title()}</h2>
  <div class="my-2 h-px bg-neutral-300 dark:bg-neutral-600"></div>
  <div class="w-fit rounded-lg border-2 p-4">
    <h3 class="text-xl">{message.resume_education_grade()}</h3>
    <p class="font-light">
      Pontificia Universidad Católica de Valparaíso ({
        message.resume_education_date
      })
    </p>
  </div>
</div>
